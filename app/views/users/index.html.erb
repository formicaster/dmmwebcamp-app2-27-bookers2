<h1>ユーザー一覧</h1>

<ul>
  <% @users.each do |user| %>
    <li><%= link_to user.name, user_path(user) %></li>
  <% end %>
</ul>

<div class='col-md-8 offset-md-1'>
  <h2>Users</h2>
    <table class='table'>
      <thead>
        <tr>
          <th>image</th>
          <th>name</th>
          <th colspan="3"></th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>
            <% @users.each do |user| %>
              <%= link_to user.name, user_path(user) %>
            <% end %>
          </td>
        </tr>
      </tbody>
    </table>
</div>